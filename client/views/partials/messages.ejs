<% if(locals.errors){ %> <% errors.forEach(function(error) { %>
<div class="messages failure">
  <%= error.msg %>
  <button type="button" class="close closeError">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% }); %> <% } %> <% if(success_msg !='' ){ %>
<div class="messages success">
  <%= success_msg %>
  <button type="button" class="close closeError">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %> <% if(error_msg !='' ){ %>
<div class="messages failure">
  <%= error_msg %>
  <button type="button" class="close closeError">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %> <% if(error !='' ){ %>
<div class="messages failure">
  <%= error %>
  <button type="button" class="close closeError">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %>
